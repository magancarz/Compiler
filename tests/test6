PROCEDURE addthree(a) IS
VAR x
BEGIN
	x := 3;
	a := a + x;
	WRITE a;
END

PROCEDURE addfive(a) IS
VAR x
BEGIN
	x := 5;
	a := a + x;
	WRITE a;
	addthree(a)
END

PROCEDURE addseven(a) IS
VAR x
BEGIN
	x := 7;
	a := a + x;
	WRITE a;
	addfive(a)
END

PROGRAM IS
VAR a, b
BEGIN
	a := 10;
	WRITE a;
	addseven(a)
	WRITE a;
END