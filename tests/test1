PROGRAM IS
VAR a, b, c, d, e
BEGIN
	a := 5;
	b := 2;
	c := 5;
	d := 8;

	IF a > b THEN
		e := 1;
		WRITE e;
	ENDIF

	IF a > c THEN
		e := 2;
		WRITE e;
	ENDIF

	IF a > d THEN
		e := 3;
		WRITE e;
	ENDIF

	IF a >= b THEN
		e := 4;
		WRITE e;
	ENDIF

	IF a >= c THEN
		e := 5;
		WRITE e;
	ENDIF

	IF a >= d THEN
		e := 6;
		WRITE e;
	ENDIF

	IF a = b THEN
		e := 7;
		WRITE e;
	ENDIF

	IF a = c THEN
		e := 8;
		WRITE e;
	ENDIF
	
	IF a = d THEN
		e := 9;
		WRITE e;
	ENDIF

	IF a <= b THEN
		e := 10;
		WRITE e;
	ENDIF

	IF a <= c THEN
		e := 11;
		WRITE e;
	ENDIF

	IF a <= d THEN
		e := 12;
		WRITE e;
	ENDIF

	IF a < b THEN
		e := 13;
		WRITE e;
	ENDIF

	IF a < c THEN
		e := 14;
		WRITE e;
	ENDIF

	IF a < d THEN
		e := 15;
		WRITE e;
	ENDIF

	IF a > b THEN
		a := a + 2;
		IF a > b THEN
			a := a + 2;
				IF a > b THEN
				a := a + 2;
			ENDIF
		ENDIF
	ENDIF
	WRITE a;
END